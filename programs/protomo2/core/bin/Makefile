include ../Makedefs


#
# make rules
#

.PHONY: help all install clean distclean depend

help:
	@ echo "usage: make [ all | install | depend | clean | distclean ]"

all:

install:
	$(INSTALL) $(INSTALLOPT) -d $(INSTROOT)/bin
	$(INSTALL) $(INSTALLOPT) -t $(INSTROOT)/bin ../make/osarch.sh


depend:

clean distclean:
