<?xml version="1.0" encoding="us-ascii"?>
<selexon>
 <range>
	<alias>range1</alias>
 </range>
 <range1>
	<name>template rotation range 1</name>
	<default>0,360,15</default>
	<description>Range of rotations for template 1</description>
	<type>int</type>
	<nargs>3</nargs>
	<limits>0,360</limits>
 </range1>
 <range2>
	<name>template rotation range 2</name>
	<default></default>
	<description>Range of rotations for template 2</description>
	<type>int</type>
	<nargs>3</nargs>
	<example>0,360,15</example>
	<limits>0,360</limits>
 </range2> 
 <range3>
	<name>template rotation range 3</name>
	<default></default>
	<description>Range of rotations for template 3</description>
	<type>int</type>
	<nargs>3</nargs>
	<example>0,360,15</example>
	<limits>0,360</limits>
 </range3>
 <maxpeaks>
	<name>maximum number of peaks</name>
	<description>Maximum number of peaks (particles) per image</description>
	<type>integer</type>
	<default>1500</default>
	<limits>1,10000</limits>
 </maxpeaks>
 <lowpass>
	<name>lowpass filter</name>
	<description>Low pass filter radius in Angstoms</description>
	<type>float</type>
	<default>0</default>
	<limits>0,1000</limits>
 </lowpass>
 <lp>
	<alias>lowpass</alias>
 </lp>
 <overlapmult>
	<name>overlap distance multiple</name>
	<description>Distance multiple for two peaks (particles) to overlap</description>
	<type>float</type>
	<default>1.5</default>
	<limits>0,10</limits>
 </overlapmult>
 <thresh>
	<name>threshold cutoff</name>
	<description>Threshold to get peaks from dogmap</description>
	<type>float</type>
	<default>0.5</default>
	<limits>0,5</limits>
 </thresh>
 <diam>
	<description>Particle diameter in angstroms used for masking template</description>
	<required>True</required>
 </diam>
 <bin>
	<required>True</required>
 </bin>
 <templateids>
	<name>template id numbers</name>
	<description>Id numbers for templates used</description>
	<type>integer</type>
	<nargs>2</nargs>
	<default></default>
	<limits>1,1000000</limits>
	<required>True</required>
 </templateids>
</selexon>
