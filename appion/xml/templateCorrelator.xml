<?xml version="1.0" encoding="us-ascii"?>
<selexon>
 <range>
	<alias>range1</alias>
 </range>
 <range1>
	<name>template rotation range 1</name>
	<default>0,360,15</default>
	<description>Range of rotations for template 1</description>
	<type>int</type>
	<nargs>3</nargs>
	<limits>0,360</limits>
 </range1>
 <range2>
	<name>template rotation range 2</name>
	<default></default>
	<description>Range of rotations for template 2</description>
	<type>int</type>
	<nargs>3</nargs>
	<example>0,360,15</example>
	<limits>0,360</limits>
 </range2> 
 <range3>
	<name>template rotation range 3</name>
	<default></default>
	<description>Range of rotations for template 3</description>
	<type>int</type>
	<nargs>3</nargs>
	<example>0,360,15</example>
	<limits>0,360</limits>
 </range3>
 <templateids>
	<name>template id numbers</name>
	<description>Id numbers for templates used</description>
	<type>integer</type>
	<nargs>2</nargs>
	<default></default>
	<limits>1,1000000</limits>
	<required>True</required>
 </templateids>
 <diam>
	<description>Particle diameter in angstroms used for masking template</description>
 </diam>
 <keepall>
	<name>keepall</name>
	<description>Keep all *.dwn.mrc</description>
	<default>False</default>
	<type>boolean</type>
 </keepall>
</selexon>
